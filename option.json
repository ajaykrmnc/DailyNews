div.svg_outer {
   display: block;
   margin-bottom: 0;
   margin-left: 0;
   margin-right: 0;
   margin-top: 0;
   padding-bottom: 0;
   padding-left: 0;
   padding-right: 0;
   padding-top: 0;
   text-align: left;
}
div.svg_inner {
   display: block;
   text-align: center;
} 
a {
  text-decoration: none;
    color: inherit;
    pointer-events: none;
}
h1 {
   text-align: justify;
   margin-bottom: 2%;
   font-size: 18px;
   font: serif;
   text-decoration: none;
   border-bottom: 2px solid black;
   padding-bottom: 2px;
}
h2 {
   font-size: 15px;
   font: serif;
   font-weight: normal;
}
h3, h4, h5, h6 {
   text-align: justify;
   font-size: 16px;
   font: serif;
   font-weight: bolder;
}

p {
  overflow-wrap: break-word;
}
dd, dt, dl {
  padding: 0;
  margin: 0;
}
img {
   display: block;
   min-height: 1em;
   max-height: 100%;
   max-width: 100%;
   padding-bottom: 0;
   padding-left: 0;
   padding-right: 0;
   padding-top: 0;
   margin-left: auto;
   margin-right: auto;
   margin-bottom: 2%;
   margin-top: 2%;
}
img.inline {
   display: block;
   min-height: 1em;
   margin-bottom: 0;
   margin-top: 0;
}
.thumbcaption {
  display: block;
  font-size: 0.9em;
  padding-right: 5%;
  padding-left: 5%;
}
hr {
   color: black;
   background-color: black;
   height: 2px;
}
table {
   width: 90%;
   border-collapse: collapse;
}
table, th, td {
   border: 1px solid black;
}
.webToEpub-author-note {
    border: 1px solid black; padding: 0.5em
}

